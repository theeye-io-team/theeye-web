<li>
  <button class="seeAgentCurl btn btn-primary simple-btn tooltiped"
    title="Check agent"
    aria-label="Center Align"
    data-customer-name="<%= customer.name %>">
    <span class="glyphicon glyphicon-download"></span>
  </button>
</li>
<li>
  <button class="editCustomer btn btn-primary simple-btn tooltiped"
    title="Edit customer"
    aria-label="Center Align"
    data-customer-id="<%= customer.id %>">
    <span class="glyphicon glyphicon-edit"></span>
  </button>
</li>
<li>
  <button class="deleteCustomer btn btn-primary simple-btn tooltiped"
    title="Delete customer"
    aria-label="Center Align"
    data-customer-id="<%= customer.id %>"
    data-customer-name="<%= customer.name %>" >
    <span class="glyphicon glyphicon-trash"></span>
  </button>
</li>
