<form id="psauxResourceForm" action="/resource" class="form-horizontal">
  <input type="hidden" name="monitor_type" value="psaux">
  <div class="form-group">
    <div class="col-sm-11">
      <h4>Host Processes Stats</h4>
    </div>
  </div>
  <div class="form-group">
    <label for="cpu" class="col-sm-3 control-label">Description</label>
    <div class="col-sm-8">
      <input data-hook="description" class="form-control" type="text" name="description" value="Host Processes">
    </div>
  </div>
  <div class="form-group">
    <label for="looptime" class="col-sm-3 control-label">Check Interval (mins)</label>
    <div class="col-sm-8">
      <select data-hook="looptime" name="looptime" class="form-control">
        <option value="0" disabled selected><strong>select check interval</strong></option>
        <% looptimes.forEach(function(t) { %>
        <option value="<%= t.id %>"><strong><%= t.value %></strong></option>
        <% }); %>
      </select>
    </div>
  </div>
</form>
<!-- end modal body -->
